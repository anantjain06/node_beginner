
<div class="container">
  <div class="row">
    <% project.forEach(function (project) {%>
    <div class="col-sm-12 col-lg-4 mb-3">
      <div class="card h-100">
        <img src="..." class="card-img-top" alt="...">
        <div class="card-body">
          <h4 class="card-title"><%=project.name%></h4>
          <h6 class="card-subtitle mb-2 text-muted"><%=project.topic%>, <%=project.date%></h6>
          <p class="card-text"><%=project.description%></p>
          <div clas="card-footer text-center">
            <a href="<%=project.hyperlink%>" class="text-center d-flex justify-content-center btn btn-primary">Visit <%=project.name%></a>
          </div>
        </div>
      </div>
    </div>
    <%})%>
  </div>
</div>

