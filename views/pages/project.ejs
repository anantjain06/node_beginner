<div class="container mt-4">
  <div class="row">
    <div class="col-sm-8">
      <article class="projectlist maincontent">
        <% project.forEach(function (project) {%>
        <div class="mb-4">
          <h4 class="projectlist-title"><%=project.title%></h4>
          <div class="projectlist-name">
            with
            <a href="/project/<%=project.shortname%>"><%=project.name%></a>
          </div>
          <div class="row projectlist-info mt-2">
            <div class="col-sm-3">
              <a href="/project/<%=project.shortname%>">
                <img
                  class="projectlist-img rounded-circle img-fluid"
                  src="/images/project/<%=project.shortname%>_tn.jpg"
                  alt="Photo of <%=project.name%>"
                />
              </a>
            </div>
            <div class="col-sm-9">
              <%=project.summary%>
            </div>
          </div>
        </div>
        <%})%>
      </article>
    </div>
    <aside class="col-sm-4">
      <%- include('./partials/artwork') %>
    </aside>
  </div>
</div>
