<div class="container mt-4">
  <div class="row">
    <div class="col-sm-8">
      <article class="projectlist maincontent">
        <% project.forEach(function (project) {%>
          <div class="mb-4">

            <h1><a href="<%=project.hyperlink%>"><%=project.name%></a></h1>

            <h2><%=project.topic%> - <%=project.date%></h2>
      
            <p><%=project.description%></p>
            
            <%=project.artwork%>

          </div>
        <%})%>
      </article>
    </div>
  </div>
</div>